{
  "name": "adadarsh23",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "description": "A modern Vite + React project by Adarsh",
  "author": "Ad Adarsh <adadarsh523@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/adadarsh23/Profile-Website"
  },
  "browserslist": "defaults",
  "prettier": {
    "semi": true,
    "singleQuote": true,
    "printWidth": 80,
    "tabWidth": 2,
    "trailingComma": "es5",
    "arrowParens": "always",
    "endOfLine": "lf"
  },
  "vitest": {
    "test": {
      "environment": "jsdom"
    }
  },
  "vite": {
    "plugins": [
      "@vitejs/plugin-react",
      "vite-plugin-compression",
      "vite-plugin-sitemap",
      "vite-plugin-remove-console",
      "vite-plugin-pwa",
      "@vitejs/plugin-legacy"
    ]
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "keywords": [
    "vite",
    "react",
    "frontend",
    "typescript",
    "web"
  ],
  "bugs": {
    "url": "https://github.com/adadarsh23/Profile-Website/issues"
  },
  "homepage": "https://github.com/adadarsh23/Profile-Website",
  "scripts": {
    "dev": "vite & npm run lighthouse-audit",
    "lighthouse-audit": "node -e \"setTimeout(()=>import('./lighthouse-audit.js'),5000)\"",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint --ext .js,.ts,.jsx,.tsx src",
    "lint:fix": "eslint --ext .js,.ts,.jsx,.tsx src --fix",
    "format": "prettier --write .",
    "type-check": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "deploy": "gh-pages -d dist",
    "clean": "rimraf dist",
    "start": "npm run dev"
  },
  "dependencies": {
    "@formspree/react": "^3.0.0",
    "@gsap/react": "^2.1.2",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-primitive": "^2.1.3",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@sentry/react": "^10.18.0",
    "@sentry/tracing": "^7.120.4",
    "@statsig/react-bindings": "^3.26.0",
    "@statsig/session-replay": "^3.26.0",
    "@statsig/web-analytics": "^3.26.0",
    "@studio-freight/lenis": "^1.0.42",
    "@tailwindcss/vite": "^4.1.14",
    "axios": "^1.12.2",
    "boxen": "^8.0.1",
    "chalk": "^5.6.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dotenv": "^17.2.3",
    "framer-motion": "^12.23.22",
    "gl-matrix": "^3.4.4",
    "gsap": "^3.13.0",
    "logrocket": "^10.1.0",
    "lucide-react": "^0.545.0",
    "motion": "^12.23.22",
    "ogl": "^1.0.11",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-ga4": "^2.1.0",
    "react-icons": "^5.5.0",
    "react-intersection-observer": "^9.16.0",
    "react-router-dom": "^7.9.3",
    "tailwind-merge": "^3.3.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.37.0",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.1",
    "@vitejs/plugin-legacy": "^7.2.1",
    "@vitejs/plugin-react": "^5.0.4",
    "chrome-launcher": "^1.2.1",
    "eslint": "^9.37.0",
    "eslint-plugin-react-hooks": "^6.1.1",
    "eslint-plugin-react-refresh": "^0.4.23",
    "gh-pages": "^6.3.0",
    "globals": "^16.4.0",
    "jsdom": "^27.0.0",
    "lighthouse": "^13.0.0",
    "postcss": "^8.5.6",
    "rollup-plugin-visualizer": "^6.0.4",
    "tailwindcss": "^4.1.14",
    "tw-animate-css": "^1.4.0",
    "vite": "^7.1.9",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-pwa": "^1.0.3",
    "vite-plugin-remove-console": "^2.2.0",
    "vite-plugin-sitemap": "^0.8.2",
    "vitest": "^3.2.4"
  }
}
