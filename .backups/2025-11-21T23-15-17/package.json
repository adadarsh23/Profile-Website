{
  "name": "adadarsh23",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "description": "A modern Vite + React project by Adarsh",
  "author": "Ad Adarsh <adadarsh523@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/adadarsh23/Profile-Website"
  },
  "browserslist": "defaults",
  "prettier": {
    "semi": true,
    "singleQuote": true,
    "printWidth": 80,
    "tabWidth": 2,
    "trailingComma": "es5",
    "arrowParens": "always",
    "endOfLine": "lf"
  },
  "vitest": {
    "test": {
      "environment": "jsdom"
    }
  },
  "vite": {
    "plugins": [
      "@vitejs/plugin-react",
      "vite-plugin-compression",
      "vite-plugin-sitemap",
      "vite-plugin-remove-console",
      "vite-plugin-pwa",
      "@vitejs/plugin-legacy"
    ]
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "keywords": [
    "vite",
    "react",
    "frontend",
    "typescript",
    "web"
  ],
  "bugs": {
    "url": "https://github.com/adadarsh23/Profile-Website/issues"
  },
  "homepage": "https://github.com/adadarsh23/Profile-Website",
  "scripts": {
    "dev": "vite && npm run lighthouse-audit",
    "lighthouse-audit": "node -e \"setTimeout(()=>import('./lighthouse-audit.js'),5000)\"",
    "build": "vite build",
    "preview": "vite preview",
    "job": "node job.js",
    "lint": "eslint --ext .js,.ts,.jsx,.tsx src",
    "lint:fix": "eslint --ext .js,.ts,.jsx,.tsx src --fix",
    "format": "prettier --write .",
    "type-check": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "deploy": "gh-pages -d dist",
    "cleandist": "rimraf dist",
    "prepare": "husky install",
    "postinstall": "husky install",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "preview-storybook": "storybook preview",
    "deploy-storybook": "storybook build && gh-pages -d storybook-static",
    "dev-tools": "node commands.cjs help",
    "clean": "node commands.cjs clean",
    "clean:cache": "node commands.cjs clean:cache",
    "fresh": "node commands.cjs fresh",
    "update:check": "node commands.cjs update:check",
    "update:all": "node commands.cjs update:all",
    "update:interactive": "node commands.cjs update:interactive",
    "update:minor": "node commands.cjs update:minor",
    "update:patch": "node commands.cjs update:patch",
    "audit": "node commands.cjs audit:check",
    "audit:fix": "node commands.cjs audit:fix",
    "analyze": "node commands.cjs analyze",
    "component": "node commands.cjs create:component",
    "page": "node commands.cjs create:page",
    "info": "node commands.cjs info",
    "help": "node commands.cjs help",
    "implementation-guide": "node IMPLEMENTATION_GUIDE.cjs",
    "enhancements": "node ENHANCEMENT_COMPLETE.cjs"
  },
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "@gsap/react": "^2.1.2",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-primitive": "^2.1.4",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-visually-hidden": "^1.2.4",
    "@react-three/drei": "^10.7.7",
    "@react-three/fiber": "^9.4.0",
    "@sentry/react": "^10.25.0",
    "@sentry/tracing": "^7.120.4",
    "@statsig/react-bindings": "^3.30.0",
    "@statsig/session-replay": "^3.30.0",
    "@statsig/web-analytics": "^3.30.0",
    "@supabase/supabase-js": "^2.81.1",
    "@tailwindcss/vite": "^4.1.17",
    "animejs": "^4.2.2",
    "axios": "^1.13.2",
    "boxen": "^8.0.1",
    "chalk": "^5.6.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "crypto": "^1.0.1",
    "dotenv": "^17.2.3",
    "express-async-handler": "^1.2.0",
    "framer-motion": "^12.23.24",
    "gl-matrix": "^3.4.4",
    "gsap": "^3.13.0",
    "lenis": "^1.3.15",
    "logrocket": "^10.1.0",
    "lucide-react": "^0.553.0",
    "mathjs": "^15.1.0",
    "mermaid": "^11.12.1",
    "motion": "^12.23.24",
    "ogl": "^1.0.11",
    "postprocessing": "^6.38.0",
    "pretty-bytes": "^7.1.0",
    "rate-limit": "^0.1.1",
    "react": "^19.2.0",
    "react-bits": "^1.0.5",
    "react-dom": "^19.2.0",
    "react-ga4": "^2.1.0",
    "react-hook-form": "^7.66.0",
    "react-icons": "^5.5.0",
    "react-intersection-observer": "^10.0.0",
    "react-markdown": "^10.1.0",
    "react-router-dom": "^7.9.6",
    "react-syntax-highlighter": "^16.1.0",
    "rehype-raw": "^7.0.0",
    "remark-gfm": "^4.0.1",
    "simplex-noise": "^4.0.3",
    "storybook": "^10.0.7",
    "tailwind-merge": "^3.4.0",
    "three": "^0.181.1",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@babel/plugin-transform-react-jsx": "^7.27.1",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.39.1",
    "@rollup/plugin-terser": "^0.4.4",
    "@storybook/addon-a11y": "^10.0.7",
    "@storybook/addon-docs": "^10.0.7",
    "@storybook/addon-onboarding": "^10.0.7",
    "@storybook/addon-vitest": "^10.0.7",
    "@storybook/react-vite": "^10.0.7",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-legacy": "^7.2.1",
    "@vitejs/plugin-react": "^5.1.1",
    "@vitest/browser": "^4.0.9",
    "@vitest/coverage-v8": "^4.0.9",
    "chrome-launcher": "^1.2.1",
    "eslint": "^9.39.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "eslint-plugin-storybook": "^10.0.7",
    "gh-pages": "^6.3.0",
    "globals": "^16.5.0",
    "husky": "^9.1.7",
    "jsdom": "^27.2.0",
    "lighthouse": "^13.0.1",
    "playwright": "^1.56.1",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "rimraf": "^6.1.0",
    "rollup-plugin-visualizer": "^6.0.5",
    "tailwindcss": "^4.1.17",
    "terser": "^5.44.1",
    "tw-animate-css": "^1.4.0",
    "vite": "^7.2.2",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-pwa": "^1.1.0",
    "vite-plugin-remove-console": "^2.2.0",
    "vite-plugin-sitemap": "^0.8.2",
    "vitest": "^4.0.9"
  }
}
